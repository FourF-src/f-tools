{
  "private": true,
  "scripts": {
    "start": "node ./server/main.js",
    "dev": "umi dev",
    "build": "npm run build:api && umi build",
    "build:api": "tsc --esModuleInterop ./api/*.ts --outDir ./server",
    "test": "umi test",
    "lint:es": "eslint --ext .js src mock tests",
    "lint:ts": "tslint \"src/**/*.ts\" \"src/**/*.tsx\"",
    "precommit": "lint-staged",
    "api": "ts-node ./api/main.ts",
    "api-dev": "nodemon -e ts -w ./api -x npm run api",
    "build:image": "npm run build && docker -t wanghsinche/qtrade:latest ."
  },
  "dependencies": {
    "axios": "^0.19.1",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "memoize-one": "^5.1.1",
    "uuid": "^3.3.3",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@antv/data-set": "^0.10.2",
    "@antv/f2": "^3.6.3",
    "@types/express": "^4.17.2",
    "@umijs/hooks": "^1.5.3",
    "antd-mobile": "^2.3.1",
    "dva": "^2.6.0-beta.6",
    "nodemon": "^2.0.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-test-renderer": "^16.0.3",
    "babel-eslint": "^9.0.0",
    "eslint": "^5.4.0",
    "eslint-config-umi": "^1.4.0",
    "eslint-plugin-flowtype": "^2.50.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "react-test-renderer": "^16.7.0",
    "ts-node": "^8.6.2",
    "tslint": "^5.12.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^3.6.0",
    "umi": "^2.7.0",
    "umi-plugin-react": "^1.8.0",
    "umi-types": "^0.3.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "git add"
    ],
    "*.{js,jsx}": [
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
